/* components/auto-input-mask/auto-input-mask.wxss */
.auto-input-mask-index{
  position: fixed;
  top: 0;left: 0;
  width: 100%;
  height: 100%;
  background-color:rgba(0, 0, 0, 0.5);
  z-index: 999;
  display: flex;
}
.content{
  width: 70%;
  height: 30%;
  background-color: #FFFFFF;
  margin: auto;
  border-radius: 20rpx;
}
.close-button{
  display: inline-block;
  margin: 20rpx 0 0 20rpx;
}
.title{
  text-align: center;
  margin-bottom: 20rpx;
}